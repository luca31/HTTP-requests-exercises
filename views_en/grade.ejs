<!doctype html>
<html>
  <head>
    <title>Grade</title>
    <link rel="stylesheet" type="text/css" href="public/style.css">
    <script src="public/script.js"></script>
  </head>
<body>
  <nav>Your score</nav>
  <header>Exercises state (<%= progress.signed?2+Object.keys(progress.ex).length:0 %>/<%= ex.length+2 %>)</header>
  <section>
    <div>
      <%= progress.signed?"User: "+progress.signed:"Not signed up yet" %>
    </div>
    <% if(Object.keys(progress.ex).length == 0 && progress.signed){ %>
      <div>
        You haven't executed exercises yet.
      </div>
    <% } else { for(let x in progress.ex){ %>
      <div>
        Exercise <%= parseInt(x)+1 %> done.
      </div>
    <% }} %>
  </section>
</body>
</html>